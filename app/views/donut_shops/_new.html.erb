
<%= form_for DonutShop.new do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />
   <%= f.label :description %>
  <%= f.text_area :description, spellcheck: "true" %><br />
  <%= f.label :address %>
  <%= f.text_field :address %><br />
  <%= f.label :phone %>
  <%= f.text_field :phone %><br />
  <%= f.label :website %>
  <%= f.text_field :website %><br />
  <%= submit_tag "Add a Donut Shop" %>
<% end %>