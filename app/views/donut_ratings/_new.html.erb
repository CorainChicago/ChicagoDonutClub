<section class="new_rating" >
     <%= form_for DonutRating.new, :url => donut_donut_ratings_path(@donut) do |f| %>
    <%= f.label :score %>
    <%= f.number_field :score %><br />
     <%= f.label :description %>
    <%= f.text_field :description %><br />
    <%= f.submit "Rate a Donut" %>
  <% end %>
</section>  


